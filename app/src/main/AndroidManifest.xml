<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jogoesiii">

    <application
        android:allowBackup="true"
        android:icon="@drawable/peixe_boca_fechada"
        android:label="Hungry Fish"
        android:roundIcon="@drawable/peixe_boca_fechada"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameOverActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SpashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>